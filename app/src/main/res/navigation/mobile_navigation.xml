<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <activity
        android:id="@+id/navigation_home"
        android:name="com.example.homedork.signup.RegisterUser"
        android:label="@string/title_home"
        tools:layout="@layout/activity_register_user" />

    <activity
        android:id="@+id/navigation_dashboard"
        android:name="com.example.homedork.signup.ForgotPassword"
        android:label="@string/title_dashboard"
        tools:layout="@layout/activity_forgot_password" />


    <activity
        android:id="@+id/navigation_games"
        android:name="com.example.homedork.test"
        android:label="@string/title_games"
        tools:layout="@layout/activity_test" />

    <activity
        android:id="@+id/navigation_notifications"
        android:name="com.example.homedork.test"
        android:label="@string/title_notifications"
        tools:layout="@layout/activity_test" />


</navigation>