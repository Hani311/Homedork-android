<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <activity
        android:id="@+id/navigation_home"
        android:name="com.example.homedork.Calendar.CalendarMain"
        android:label="@string/title_home"
        tools:layout="@layout/activity_calandar" />

    <activity
        android:id="@+id/navigation_dashboard"
        android:name="com.example.homedork.Game"
        android:label="@string/title_dashboard"
        tools:layout="@layout/activity_game" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.homedork.Settings.SettingsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/activity_vibrate" />
</navigation>